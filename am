exit

cmd_detection() {
# command -v "cmd", alias "cmd" and which "cmd"
var='which cmd'
if grep -q 'not found'
cmd not found
else if grep 'built-in'
cmd is a built in
else if grep 'aliased to'
cmd is an alias
var='command -v cmd'
if $var = cmd
cmd is a function
else if grep /
cmd is a command
else
what is cmd?
}

create/read/update/delete
# add/ls/edit/rm
create(){
if exists ...
echo ... >> .zshrc
}

read(){
if exists
echo it
else doesen't exist
}

update(){
if exists
sed ...
else echo doesn't exist
}

delete(){
if alias grep alias
else if func grep func
else doesn't exist
rm alias or func
}